                <a href="#">홈페이지</a>
                <a href="#" id="login">로그인 / 회원가입</a>
                <a href="#">학습</a>
                <a href="#">My page</a>
                <a href="#">메뉴</a>
            </div>
        </div>
        <!-- topLongContainer 끝 -->

        <!-- 가운데 내용들 본격적인 시작 -->
        <div id="contentsContainer" class="bg_cont">
            <center><h1>회원가입</h1></center>

            <!-- 가운데 회원가입란 form 시작 -->
            <div id="contentsContainerInnerRegisterForm" class="bg_cont">
                <div id="contentsContainerInnerRegisterForm_Form">
                    <!--userid-->
                    <div class="innerForm_input">
                        <p>USER ID</p>
                        <div id="user">
                            <input type="text" id="userid" placeholder="USER ID"/>
                            <!--userid 중복확인버튼-->
                            <button type="duplicate_btn">확인</button>
                        </div>
                    </div>
                    <div class="innerForm_input">
                        <!--userpw-->
                        <p>USER PW</p>
                        <input type="password" id="userpw" placeholder="USER PW"/>
                    </div>
                    
                    <div class="innerForm_input">
                        <!--username-->
                        <p>USER NAME</p>
                        <input type="text" id="username" placeholder="USER NAME"/>
                    </div>
                    
                    <div class="innerForm_input">
                        <!--email-->
                        <p>EMAIL</p>
                        <div id="user">
                            <input type="email" id="useremail" placeholder="EMAIL"/>
                            <!--email 중복확인버튼-->
                            <button type="duplicate_btn">확인</button>
                        </div>
                    </div>
                    
                    <div class="innerForm_input">
                        <!--phone number-->
                        <p>PHONE NUMBER</p>
                        <div id="user">
                            <input type="text" id="phonenumber" placeholder="PHONE NUMBER"/>
                            <!--phone number 중복확인버튼-->
                            <button type="duplicate_btn">확인</button>
                        </div>
                    </div>
                    
                    <p id="agree">* 회원가입시 개인정보제공 동의로 간주됩니다 *</p>
                </div>
                <!--contentsContainerInnerRegisterForm 끝-->
            </div>
        </div>

        <div id="under_innerForm_registBtn">
            <div id="under_innerForm_blueDiv_inner_semiContainer">
                <center>
                    <button type="button" class="under_innerForm_blueDiv_innerBtn">가입하기</button>
                </center>
            </div>
        </div>
    </form>
